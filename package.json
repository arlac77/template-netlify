{
  "scripts": {
    "build": "rollup -c tests/app/rollup.config.mjs"
  },
  "template": {
    "usedBy": [
      "arlac77/svelte-guard-history-router",
      "arlac77/svelte-session-manager"
    ],
    "mergers": [
      {
        "type": "Readme",
        "pattern": "README.md",
        "options": {
          "badges": [
            {
              "name": "Netlify Status",
              "icon": "https://api.netlify.com/api/v1/badges/{{netlifly.site.id}}/deploy-status",
              "url": "https://app.netlify.com/sites/{{name}}/deploys",
              "order": 0.11
            }
          ]
        }
      }
    ]
  }
}